  },
  "components": {
    "schemas": {
      "Movie": {
        "required": [
          "stock",
          "title"
        ],
        "type": "object",
        "properties": {
          "movieID": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "maxLength": 200,
            "minLength": 1,
            "type": "string"
          },
          "stock": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Person": {
        "required": [
          "membershipType",
          "userName"
        ],
        "type": "object",
        "properties": {
          "userID": {
            "type": "integer",
            "format": "int32"
          },
          "userName": {
            "maxLength": 100,
            "minLength": 1,
            "type": "string"
          },
          "membershipType": {
            "maxLength": 20,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "Rental": {
        "required": [
          "movieID",
          "rentalDate",
          "status",
          "userID"
        ],
        "type": "object",
        "properties": {
          "rentalID": {
            "type": "integer",
            "format": "int32"
          },
          "userID": {
            "type": "integer",
            "format": "int32"
          },
          "user": {
            "$ref": "#/components/schemas/Person"
          },
          "movieID": {
            "type": "integer",
            "format": "int32"
          },
          "movie": {
            "$ref": "#/components/schemas/Movie"
          },
          "status": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "rentalDate": {
            "type": "string",
            "format": "date-time"
          },
          "returnDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}