{
  "openapi": "3.0.1",
  "info": {
    "title": "RentalManagement",
    "version": "1.0"
  },
  "paths": {
    "/api/Rentals": {
      "post": {
        "tags": [
          "Rentals"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Rental"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/Rental"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/Rental"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
